{
  "name": "pollen-solutions/cookie",
  "description": "Pollen Cookie Component - Cookies repository",
  "homepage": "https://www.presstify.com/pollen-solutions/cookie/",
  "keywords": [
    "pollen-solutions",
    "component",
    "cookie"
  ],
  "time": "2021-02-24",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com",
      "role": "Developper"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.presstify.com/pollen-solutions/"
    }
  ],
  "require": {
    "php": ">=7.4",
    "composer/composer": "^2.0",
    "composer/installers": "~1.0",
    "pollen-solutions/encryption": "^1.0",
    "pollen-solutions/http": "^1.0",
    "pollen-solutions/support": "^1.0",
    "pollen-solutions/validation": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\Cookie\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\Cookie\\Test\\": "tests"
    }
  },
  "suggest": {
    "pollen-solutions/container" : "Pollen Container Component - PSR-11 ready Dependencies Injection Container",
    "pollen-solutions/routing" : "Pollen Routing Component - HTTP request mapping and HTTP response resolution"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
